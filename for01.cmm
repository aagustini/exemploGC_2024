int i;

void main() {

    for (i=0; i<10; i++) {
        if (i==3) continue;
        if (i==4) continue;
        if (i==7) break;

        write("i: ", i);
    }

    i = 10;
    for (;;){
        i = i + 1;
        if (i==13) continue;
        if (i==14) continue;
        if (i==20) break;

        write("i: ", i);

    }
}